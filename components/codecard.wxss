.code-card {
  margin: 20px;
  background-color: #FAFAFA;
  border: 1px solid #EEE;
  border-radius: 5px;
}
.code-path {
  padding: 5px;
  border-bottom: 1px solid #EEE;
}
.code-path > text {
  font-size: 14px;
  font-weight: 400;
  color: #333;
}
.code-data {
  padding: 5px;
}
.code-data > text {
  font-size: 12px;
  color: #666;
  font-weight: 300;
  line-height: 22px;
}